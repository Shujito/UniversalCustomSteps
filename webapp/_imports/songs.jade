for group in groups
	.container
		.row
			.col-md-12
				.panel.panel-default
					.panel-headign.text-center
						h2 #{ group.name }
					.panel-body.text-center
						p.text-muted #{ group.songs.length + ' songs' }
		.row
			for song in group.songs
				.col-sm-6.col-md-4.col-lg-3
					.list-group
						a.list-group-item(href='#{ song.id.toString(16) }')
							h4.list-group-item-heading #{ song.name }
							p.list-group-item-text #{ song.artist }
